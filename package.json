  },
  "devDependencies": {
    "@types/chai": "4.1.1",
    "@types/mocha": "2.2.46",
    "@types/node": "9.3.0",
    "@types/sax": "1.0.0",
    "chai": "4.1.2",
    "coveralls": "3.0.0",
    "mocha": "5.0.0",
    "nyc": "11.4.1",
    "prettier": "1.10.2",
    "should": "13.2.1",
    "ts-node": "4.1.0",
    "tslint": "5.11.0",
